  "scenario_params": {
    "tls": {
      "initcwnd_bytes": 14336,
      "cert_chain_levels": 2
    },
    "jwt": {
      "http_header_limit_bytes": 8192,
      "sign_verify_ratio": 1000000
    },
    "code_signing": {
      "binaries_per_app": 20
    },
    "dnssec": {
      "udp_limit_bytes": 512,
      "edns_limit_bytes": 1232,
      "base_response_bytes": 80
    }
